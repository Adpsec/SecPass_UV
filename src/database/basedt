
   public class Usuario

   {





	   private int idUser;
	   public final int getIdUser()
	   {
		   return idUser;
	   }
	   public final void setIdUser(int value)
	   {
		   idUser = value;
	   }



	   private String name;
	   public final String getName()
	   {
		   return name;
	   }
	   public final void setName(String value)
	   {
		   name = value;
	   }



	   private String user;
	   public final String getUser()
	   {
		   return user;
	   }
	   public final void setUser(String value)
	   {
		   user = value;
	   }



	   private byte[] pswd;

	   public final byte[] getPswd()
	   {
		   return pswd;
	   }

	   public final void setPswd(byte[] value)
	   {
		   pswd = value;
	   }



	   private String salt;
	   public final String getSalt()
	   {
		   return salt;
	   }
	   public final void setSalt(String value)
	   {
		   salt = value;
	   }

   }
