package cifradohash.data;

public class CypherHashContext extends DbContext
{
	public CypherHashContext(DbContextOptions<CypherHashContext> options)
	{
		super(options);
	}

	private DbSet<Usuario> Usuarios;
	public final DbSet<Usuario> getUsuarios()
	{
		return Usuarios;
	}
	public final void setUsuarios(DbSet<Usuario> value)
	{
		Usuarios = value;
	}
}
